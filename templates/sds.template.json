      "file": { "type": "keyword" },
      "folder": { "type": "keyword" },
      "id": { "type": "keyword" },
      "internal_product": { "type": "keyword" },
      "logtype": { "type": "keyword" },
      "msg": { "type": "keyword" },
      "severity": { "type": "keyword" },
      "src": { "type": "keyword" },
      "srcname": { "type": "keyword" },
      "type": { "type": "keyword" },
      "user": { "type": "keyword" },
      "userFullName": { "type": "keyword" }
    }
  }
}
